<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="upc.genweb.soa">

   <browser:page
      for="*"
	    name="gn6-alta-tiquet"
        layer="upc.genwebCBL.interfaces.ISOALayer"
	    class=".altaTiquet.AltaTiquetView"
	    permission="zope2.View"
   />
   <!--
       Atenció: aquestes dues declaracions son utilitzades dins de
                dadesUsuari.py

                Si es modifica el nom de la vista cal actualitzar el fitxer
                dadesUsuari.py
    -->
   <browser:page
      for="*"
        name="gn6-recollir-dades"
        layer="upc.genwebCBL.interfaces.ISOALayer"
        class=".dadesUsuari.DadesUsuariView"
        permission="zope2.View"
   />
   <!-- #TODO la següent página només hauria de ser visible pels
        gestors
    -->
   <browser:page
      for="*"
        name="gn6-helper-url-recollir"
        layer="upc.genwebCBL.interfaces.ISOALayer"
        class=".dadesUsuari.HelperView"
        permission="zope2.View"
   />
   <!--
        fi
    -->

    <!-- GuiaDocent -->
    <browser:page
      for="*"
        name="guiadocent-obtenir-pdf"
        layer="upc.genwebCBL.interfaces.ISOALayer"
        class=".guiaDocent.ObtenirPDFView"
        permission="zope2.View"
   />

</configure>
